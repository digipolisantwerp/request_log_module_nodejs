{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "max-len": ["error", { "code": 150 }],
        "prefer-destructuring": ["error", {"object": true, "array": false}],
        "no-underscore-dangle": 0
    }
}
